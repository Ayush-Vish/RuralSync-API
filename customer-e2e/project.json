{
  "name": "customer-e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["customer"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
